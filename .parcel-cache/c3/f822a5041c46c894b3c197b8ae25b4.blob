const init = function () {
  //we will export the function from recipeView and add it here
  _recipeView.default.addHandlerRender(controlRecipe); //Search


  _searchView.default.addHandlerSearch(controlSearchResults); //page button


  _paginationView.default.addHandlerClick(controlPagination); //serving button


  controlServing();
};

